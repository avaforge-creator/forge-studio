# Vercel Environment Variables
# Copy this to .env and fill in your values

# OpenClaw (optional - for server-side polling)
OPENCLAW_API_KEY=your_api_key_here

# Local tunnel URL (for fetching data from your machine)
# Keep polling running locally, Vercel fetches from here
LOCAL_TUNNEL_URL=https://your-tunnel-url.loca.lt

# Vercel KV (optional - for storing data in the cloud)
# KV_URL=your_kv_url_here
